    "@atproto/xrpc-server": "^0.3.2",
    "better-sqlite3": "^8.3.0",
    "cron": "^3.1.7",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "kysely": "^0.22.0",
    "multiformats": "^9.9.0"
  },
  "devDependencies": {
    "@mkizka/eslint-config": "^4.1.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/better-sqlite3": "^7.6.4",
    "@types/express": "^4.17.17",
    "@types/node": "^20.1.2",
    "eslint": "^9.0.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "prettier-plugin-pkg": "^0.18.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4"
  },
  "lint-staged": {
    "*": [
